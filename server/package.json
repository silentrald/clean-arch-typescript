{
  "name": "node-sample",
  "version": "1.0.0",
  "main": "app.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register src/app",
    "build": "tsc",
    "start": "node dist/app",
    "devtest": "nodemon -e ts,spec.ts --exec 'yarn test && ts-node -r tsconfig-paths/register' src/app",
    "test": "yarn test:unit && yarn test:int",
    "test:unit": "jest --color --coverage --maxWorkers=4 --testPathPattern=\".test.ts$\"",
    "test:int": "NODE_ENV=test yarn ts-node -r tsconfig-paths/register jest.setup",
    "db-up": "ts-node -r tsconfig-paths/register src/migrations/_core/cmd up",
    "db-insert": "ts-node -r tsconfig-paths/register src/migrations/_core/cmd ins",
    "db-delete": "ts-node -r tsconfig-paths/register src/migrations/_core/cmd del",
    "db-down": "ts-node -r tsconfig-paths/register src/migrations/_core/cmd down",
    "db-up:prod": "node dist/migrations/_core/cmd up",
    "db-reset": "yarn db-down && yarn db-up && yarn db-insert"
  },
  "dependencies": {
    "ajv": "^8.6.2",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "babel-jest": "^27.0.6",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "chokidar": "^3.5.3",
    "connect-redis": "^5.1.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "ioredis": "^4.24.2",
    "knex": "^0.95.15",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "nodemailer": "^6.5.0",
    "pg": "^8.5.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.6.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.7",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/chokidar": "^2.1.3",
    "@types/connect-redis": "^0.0.16",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/csurf": "^1.11.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/ioredis": "^4.22.0",
    "@types/jest": "^26.0.24",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.1",
    "@types/pg": "^7.14.11",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.6.3"
  },
  "_moduleAliases": {
    "@db": "dist/db",
    "@entities": "dist/entities",
    "@helpers": "dist/helpers",
    "@middlewares": "dist/middlewares",
    "@modules": "dist/modules",
    "@use-cases": "dist/use-cases",
    "@app": "dist/app",
    "@config": "dist/config"
  }
}
